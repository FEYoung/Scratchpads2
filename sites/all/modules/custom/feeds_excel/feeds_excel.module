<?php

/**
 * Implementation of hook_feeds_plugins().
 */
function feeds_excel_feeds_plugins(){
  $info = array();
  $info['FeedsExcelParser'] = array(
    'name' => 'Excel parser',
    'description' => 'Parses an excel file.',
    'handler' => array(
      'parent' => 'FeedsParser',
      'class' => 'FeedsExcelParser',
      'file' => 'FeedsExcelParser.inc',
      'path' => drupal_get_path('module', 'feeds_excel')
    )
  );
  return $info;
}

/**
 * Implementation of hook_menu().
 */
function feeds_excel_menu(){
  return array(
    'import/%feeds_importer/template' => array(
      'title' => 'Excel template',
      'page callback' => 'feeds_excel_download_template',
      'page arguments' => array(
        1
      ),
      'access arguments' => array(
        'access content'
      ),
      'file' => 'feeds_excel.template.inc',
      'type' => MENU_CALLBACK
    )
  );
}