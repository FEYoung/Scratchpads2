<?php

/**
 * @file
 * Imports data from an external database.
 */
/**
 * Implements hook_ctools_plugin_api().
 */
function feeds_imu_specimens_ctools_plugin_api($owner, $api){
  if($owner == 'feeds' && $api == 'plugins'){return array(
      'version' => 1
    );}
}

/**
 * Implements hook_ctools_plugin_type().
 */
function feeds_imu_specimens_ctools_plugin_type(){
  return array(
    'plugins' => array(
      'cache' => TRUE,
      'use hooks' => TRUE,
      'classes' => array(
        'handler'
      )
    )
  );
}

/**
 * Implements hook_feeds_plugins().
 */
function feeds_imu_specimens_feeds_plugins(){
  $info = array();
  $info['FeedsIMUspecimensFetcher'] = array(
    'name' => 'IMU specimens fetcher',
    'description' => 'Fetches data from a KEMU eCatalog module.',
    'handler' => array(
      'parent' => 'FeedsFetcher',
      'class' => 'FeedsIMUspecimensFetcher',
      'file' => 'FeedsIMUspecimensFetcher.inc',
      'path' => drupal_get_path('module', 'feeds_imu_specimens') . '/plugins'
    )
  );
  $info['FeedsIMUspecimensParser'] = array(
    'name' => 'IMU specimens parser',
    'description' => 'Parses data fetched from a KEMU eCatalog module.',
    'handler' => array(
      'parent' => 'FeedsParser',
      'class' => 'FeedsIMUspecimensParser',
      'file' => 'FeedsIMUspecimensParser.inc',
      'path' => drupal_get_path('module', 'feeds_imu_specimens') . '/plugins'
    )
  );
  return $info;
}