<?php

function feeds_imu_fetcher_menu_alterOLD(&$items){
  $items['admin/content/specimen_observation22']['access callback'] = 'user_access';
  $items['admin/content/specimen_observation22']['access arguments'] = array(
    'scratchpad team'
  );
}

function feeds_imu_fetcher_node_access($node, $op, $account){
  $type = is_string($node) ? $node : $node->type;
  print "op=".$op;
  // Deny updating the content if it's a specimen_observation
  if($type == 'specimen_observation' && $node->status && $op == 'update'){
    drupal_set_message('You may not make changes to it.', 'error');
    print "op=".$op;
   // return NODE_ACCESS_DENY;
  }
}

/**
 * @file
 * Imports data from an external database.
 */
/**
 * Implements hook_ctools_plugin_api().
 */
function feeds_imu_fetcher_ctools_plugin_api($owner, $api){
  if($owner == 'feeds' && $api == 'plugins'){return array(
      'version' => 1
    );}
}

/**
 * Implements hook_ctools_plugin_type().
 */
function feeds_imu_fetcher_ctools_plugin_type(){
  return array(
    'plugins' => array(
      'cache' => TRUE,
      'use hooks' => TRUE,
      'classes' => array(
        'handler'
      )
    )
  );
}

/**
 * Implements hook_feeds_plugins().
 */
function feeds_imu_fetcher_feeds_plugins(){
  $info = array();
  $info['FeedsIMUFetcher'] = array(
    'name' => 'IMU fetcher',
    'description' => 'Fetches data from KEMU.',
    'handler' => array(
      'parent' => 'FeedsFetcher',
      'class' => 'FeedsIMUFetcher',
      'file' => 'FeedsIMUFetcher.inc',
      'path' => drupal_get_path('module', 'feeds_imu_fetcher') . '/plugins'
    )
  );
  $info['FeedsXPathParserMemory'] = array(
    'name' => t('XPath XML extended'),
    'description' => t('Parse XML using XPath with extended memory.'),
    'handler' => array(
      'parent' => 'FeedsParser',
      'class' => 'FeedsXPathParserMemory',
      'file' => 'FeedsXPathParserMemory.inc',
      'path' => drupal_get_path('module', 'feeds_imu_fetcher') . '/plugins'
    )
  );
  return $info;
}



