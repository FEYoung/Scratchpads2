<?php

/**
 * Add a file field to biblio
 */
function scratchpads_biblio_update_7001(){
  module_load_include('inc', 'scratchpads_biblio', 'scratchpads_biblio.features.field');
  $fields = scratchpads_biblio_field_default_fields();
  foreach($fields as $field){
    field_create_instance($field[field_instance]);
  }
  return 'File field added';
}


